let main () =	
	let ns = [|1;15;14;2;13;12;6;9;5;4;10;3;11;8|] in	
	let t = T (1, 7, E, E) in	
	let t = Array.fold_left (fun t k -> 
		let _ = fprintf outch "env:newtest\t\n" in
		insert k t
		) t ns in
	let _ = Array.fold_left (fun t _ -> 
		let _ = fprintf outch "env:newtest\t\n" in
		if (Random.bool ()) then (ignore (find_min t); t)
		else snd (delete_min t)
		) t ns in
	()
let _ = main () 